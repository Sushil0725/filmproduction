# Server configuration
PORT=4000
NODE_ENV=development

# Admin credentials (update these!)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=change-me
# Alternatively, you can provide a bcrypt hash instead of ADMIN_PASSWORD
# ADMIN_PASSWORD_HASH=$2a$10$...

# JWT configuration
JWT_SECRET=please-change-this-secret
JWT_EXPIRES_IN=2h


# CORS
CORS_ORIGIN=*

# Optional: custom data directory (defaults to ./datas inside backend)
# DATA_DIR=
